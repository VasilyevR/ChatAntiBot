{
  "type": "project",
  "name": "vasilyevr/spammer-kicker-telegram-bot",
  "description": "Spammer kicker Telegram Bot",
  "keywords": ["telegram", "bot", "antispam", "ban"],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "VasilyevR",
      "email": "romanvasilyev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-intl": "^7.3",
    "ext-sqlite3": "^7.3",
    "guzzlehttp/guzzle": "^7.2",
    "hassankhan/config": "^2.2",
    "http-interop/http-factory-guzzle": "^1.0",
    "monolog/monolog": "^2.2",
    "tg-bot-api/bot-api-base": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Tests\\": "tests/"
    }
  }
}
